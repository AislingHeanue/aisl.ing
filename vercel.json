{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "env": {
    "PATH": "/usr/local/go/bin:$PATH"
  },
  "buildCommand": "dnf update -y -q && dnf install golang -y -q && npm run build",
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
