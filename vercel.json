{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/particles",
      "destination": "market://details?id=ing.aisl.particle_game",
      "permanent": true,
      "has": [
        {
          "type": "header",
          "key": "User-Agent",
          "value": ".*Android.*"
        }
      ]
    },
    {
      "source": "/particles",
      "destination": "https://github.com/AislingHeanue/particle_game",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "buildCommand": "sh vercel.sh"
}
